{
  "version": 2,
  "builds": [
    {
      "src": "conference_room_booking/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "conference_room_booking/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "conference_room_booking.settings"
  }
}
